*{outline: none;}
body { background : #007fa0;
 margin:0px;
 font-size:16px;
 color: #fff; 
 font-family: &quot;Lucida sans unicode&quot;, &quot;Lucida sans&quot;, sans-serif;
 }
#outer-wrapper {max-width:768px;width:100%;margin:10px auto ;padding:10px;background-color: #007fa0;}
#content-wrapper,#search-wrapper{border-bottom: 5px dashed #fff;}
a {text-decoration:none;}
a:link {
 color:#000000;
 text-decoration:none;
 }
a:visited {
 color:#000000;
 text-decoration:none;
 }
a:hover { 
 color: #1aa1c5;
