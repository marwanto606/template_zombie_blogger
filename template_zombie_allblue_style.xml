h5 {font-size:10px;}
h6 {font-size:8px;}
hr {background-color:#fff;border:0;height:1px;margin-bottom:2px;clear:both;}

#header-wrapper {
 width: 100%; 
 }

#header {
 color: #007fa0;
 text-align: center;
 }

#header h2 {
 font-size: 30px;
 text-transform:uppercase;
